<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uk.co.acleaninstall.essentialtools">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.PhoneInfoActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity
            android:name=".activity.BuildInfoActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity
            android:name=".activity.SimInfoActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity
            android:name=".activity.HiLoGameActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity
            android:name=".activity.ShellCommandsActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity
            android:name=".activity.OtherToolsActivity"
            android:label=""
            android:parentActivityName=".activity.MainActivity"/>
        <activity android:name=".activity.RootInfoActivity"></activity>
    </application>

</manifest>